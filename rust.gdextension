[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust/target/debug/libgodot_dash_v2.so"
linux.release.x86_64 =   "res://rust/target/release/libgodot_dash_v2.so"
windows.debug.x86_64 =   "res://rust/target/debug/godot_dash_v2.dll"
windows.release.x86_64 = "res://rust/target/release/godot_dash_v2.dll"
macos.debug =            "res://rust/target/debug/libgodot_dash_v2.dylib"
macos.release =          "res://rust/target/release/libgodot_dash_v2.dylib"
macos.debug.arm64 =      "res://rust/target/debug/libgodot_dash_v2.dylib"
macos.release.arm64 =    "res://rust/target/release/libgodot_dash_v2.dylib"